#!/home/ken/.local/bin/python3.5
# EASY-INSTALL-ENTRY-SCRIPT: 'avendesora','console_scripts','avendesora'
__requires__ = 'avendesora'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('avendesora', 'console_scripts', 'avendesora')()
    )
