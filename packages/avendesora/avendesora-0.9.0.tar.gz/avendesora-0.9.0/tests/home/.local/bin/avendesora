#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'avendesora==0.8.1','console_scripts','avendesora'
__requires__ = 'avendesora==0.8.1'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('avendesora==0.8.1', 'console_scripts', 'avendesora')()
    )
