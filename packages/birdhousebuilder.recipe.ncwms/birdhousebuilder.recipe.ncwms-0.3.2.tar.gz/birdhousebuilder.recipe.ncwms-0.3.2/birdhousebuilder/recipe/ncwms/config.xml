<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<config>
  <datasets/>
  <cache enabled="${enablecache}">
    <inMemorySizeMB>256</inMemorySizeMB>
    <elementLifetimeMinutes>0.0</elementLifetimeMinutes>
  </cache>
  <dynamicServices>
    <dynamicService alias="outputs" servicePath="${data_dir}" datasetIdMatch=".*" dataReaderClass="" copyrightStatement="" moreInfoUrl="" disabled="false" queryable="true" downloadable="false"/>
    <dynamicService alias="cache" servicePath="${data2_dir}" datasetIdMatch=".*" dataReaderClass="" copyrightStatement="" moreInfoUrl="" disabled="false" queryable="true" downloadable="false"/>
  </dynamicServices>
  <contact>
    <name>${contact}</name>
    <organization>${organization}</organization>
    <telephone></telephone>
    <email>${email}</email>
  </contact>
  <server>
    <title>${title}</title>
    <allowFeatureInfo>true</allowFeatureInfo>
    <maxImageWidth>1024</maxImageWidth>
    <maxImageHeight>1024</maxImageHeight>
    <abstract>${abstract}</abstract>
    <keywords>${keywords}</keywords>
    <url>${url}</url>
    <allowglobalcapabilities>${allowglobalcapabilities}</allowglobalcapabilities>
  </server>
</config>


