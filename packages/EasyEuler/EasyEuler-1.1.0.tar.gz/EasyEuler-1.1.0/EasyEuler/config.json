{
    "filename_format": "euler_{id:0>3}.{extension}",
    "default_language": "python",
    "languages": {
        "python":
        {
            "extension": "py",
            "command": "python {path}",
            "template": "python"
        },
        "c": {
            "extension": "c",
            "command": "gcc -o {path}.out {path} && ./{path}.out && rm {path}.out",
            "template": "c"
        },
        "ruby": {
            "extension": "rb",
            "command": "ruby {path}",
            "template": "ruby"
        },
        "javascript": {
            "extension": "js",
            "command": "node {path}",
            "template": "javascript"
        },
        "haskell": {
            "extension": "hs",
            "command": "runhaskell {path}",
            "template": "haskell"
        },
        "c++": {
            "extension": "cpp",
            "command": "gcc -o {path}.out {path} && ./{path}.out && rm {path}.out",
            "template": "c"
        }
    }
}
