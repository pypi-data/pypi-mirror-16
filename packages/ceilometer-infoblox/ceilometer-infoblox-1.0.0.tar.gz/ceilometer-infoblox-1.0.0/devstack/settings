CEILOMETER_INFOBLOX_DIR=$DEST/ceilometer-infoblox

CEILOMETER_INFOBLOX_SNMP_COMMUNITY=${CEILOMETER_INFOBLOX_SNMP_COMMUNITY:-public}
CEILOMETER_INFOBLOX_USE_FLOATING_IP=${CEILOMETER_INFOBLOX_USE_FLOATING_IP:-True}
CEILOMETER_INFOBLOX_MANAGEMENT_NETWORK=${CEILOMETER_INFOBLOX_MANAGEMENT_NETWORK:-private}

enable_service ceilometer-infoblox
