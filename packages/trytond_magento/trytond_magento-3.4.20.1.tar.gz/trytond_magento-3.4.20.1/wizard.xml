<tryton>
    <data>

        <!--Configuration Wizard-->
        <record model="ir.action.wizard" id="wizard_configure_magento">
            <field name="name">Configure Magento Connection</field>
            <field name="wiz_name">magento.wizard_configure_magento</field>
            <field name="model">sale.channel</field>
        </record>
        <record model="ir.action.keyword" id="wizard_configure_magento_keyword">
            <field name="keyword">form_action</field>
            <field name="model">sale.channel,-1</field>
            <field name="action" ref="wizard_configure_magento"/>
        </record>

        <!--Test Connection Wizard-->
        <record model="ir.ui.view" id="wizard_test_magento_connection_view_form">
            <field name="model">magento.wizard_test_connection.start</field>
            <field name="type">form</field>
            <field name="name">wizard_test_connection_start_form</field>
        </record>

        <!--Import Websites Wizard-->
        <record model="ir.ui.view" id="wizard_import_websites_view_form">
            <field name="model">magento.wizard_import_websites.start</field>
            <field name="type">form</field>
            <field name="name">wizard_import_websites_start_form</field>
        </record>

        <!--Import Websites Stores-->
        <record model="ir.ui.view" id="wizard_import_stores_view_form">
            <field name="model">magento.wizard_import_stores.start</field>
            <field name="type">form</field>
            <field name="name">wizard_import_stores_start_form</field>
        </record>

        <!--Configuration Success / Failure Wizard-->
        <record model="ir.ui.view" id="wizard_configuration_success_view_form">
            <field name="model">magento.wizard_configuration_success.start</field>
            <field name="type">form</field>
            <field name="name">wizard_configuration_success_start_form</field>
        </record>

        <record model="ir.ui.view" id="wizard_configuration_failure_view_form">
            <field name="model">magento.wizard_configuration_failure.start</field>
            <field name="type">form</field>
            <field name="name">wizard_configuration_failure_start_form</field>
          </record>

        <!--Export Shipment Status Wizard-->
        <record model="ir.action.wizard" id="wizard_export_magento_shipment_status">
            <field name="name">Export Shipment Status To Magento</field>
            <field name="wiz_name">magento.wizard_export_shipment_status</field>
            <field name="model">sale.channel</field>
        </record>
        <record model="ir.action.keyword" id="wizard_export_magento_shipment_status_keyword">
            <field name="keyword">form_action</field>
            <field name="model">sale.channel,-1</field>
            <field name="action" ref="wizard_export_magento_shipment_status"/>
        </record>
        <record model="ir.ui.view" id="wizard_export_magento_shipment_status_view_start_form">
            <field name="model">magento.wizard_export_shipment_status.start</field>
            <field name="type">form</field>
            <field name="name">export_shipment_status_start_form</field>
        </record>

        <!--Export Data Configure Wizard-->
        <record model="ir.ui.view" id="export_data_configure_view_form">
            <field name="model">sale.channel.export_data.configure</field>
            <field name="type">form</field>
            <field name="name">wizard_export_data_configure_form</field>
        </record>

        <!--Update Magento Product Catalog Wizard-->
        <record model="ir.action.wizard" id="wizard_magento_update_catalog">
            <field name="name">Update Magento Catalog</field>
            <field name="wiz_name">magento.update_catalog</field>
            <field name="model">sale.channel</field>
        </record>
        <record model="ir.action.keyword" id="wizard_magento_update_catalog_keyword">
            <field name="keyword">form_action</field>
            <field name="model">sale.channel,-1</field>
            <field name="action" ref="wizard_magento_update_catalog"/>
        </record>
        <record model="ir.ui.view" id="magento_update_catalog_start_view_form">
            <field name="model">magento.update_catalog.start</field>
            <field name="type">form</field>
            <field name="name">update_catalog_start_form</field>
        </record>
    </data>
</tryton>
