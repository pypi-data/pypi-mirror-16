#!/usr/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'gapi4term==0.8.3','console_scripts','gapi'
__requires__ = 'gapi4term==0.8.3'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('gapi4term==0.8.3', 'console_scripts', 'gapi')()
    )
