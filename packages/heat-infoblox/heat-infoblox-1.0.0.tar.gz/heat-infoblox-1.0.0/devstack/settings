HEAT_INFOBLOX_DIR=$DEST/heat-infoblox

HEAT_INFOBLOX_API_IP=${HEAT_INFOBLOX_API_IP:-172.16.98.100}
HEAT_INFOBLOX_WAPI_URL=${HEAT_INFOBLOX_WAPI_URL:-https://$HEAT_INFOBLOX_API_IP/wapi/v2.2.1/}
HEAT_INFOBLOX_USERNAME=${HEAT_INFOBLOX_USERNAME:-admin}
HEAT_INFOBLOX_PASSWORD=${HEAT_INFOBLOX_PASSWORD:-infoblox}
HEAT_INFOBLOX_SSLVERIFY=${HEAT_INFOBLOX_SSLVERIFY:-False}

enable_service heat-infoblox
