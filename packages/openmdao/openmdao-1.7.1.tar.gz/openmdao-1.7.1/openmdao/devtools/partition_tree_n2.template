        var n2ElementsGroup = n2Group.append("g"),
            n2GridLinesGroup = n2Group.append("g"),
            n2ComponentBoxesGroup = n2Group.append("g"),
            n2ArrowsGroup = n2Group.append("g"),
            n2DotsGroup = n2Group.append("g");


        function DrawBorder(g,u,v,color,justUpdate){
            var shape1 = justUpdate ? g.select(".bordR1") : g.append("rect").attr("class", "bordR1").style("fill", color);
            var shape2 = justUpdate ? g.select(".bordR2") : g.append("rect").attr("class", "bordR2").style("fill", color);
            var shape3 = justUpdate ? g.select(".bordR3") : g.append("rect").attr("class", "bordR3").style("fill", color);
            var shape4 = justUpdate ? g.select(".bordR4") : g.append("rect").attr("class", "bordR4").style("fill", color);

            shape1.attr("x", -u).attr("y", -v).attr("width", u * 2).attr("height", v * .2);
            shape2.attr("x", -u).attr("y", -v).attr("width", u * .2).attr("height", v * 2);
            shape3.attr("x", u * .8).attr("y", -v).attr("width", u * .2).attr("height", v * 2);
            shape4.attr("x", -u).attr("y", v * .8).attr("width", u * 2).attr("height", v * .2);
        }
        function DrawS_S(g,u,v,color,justUpdate){
            var shape = justUpdate ? g.select(".ssMid") : g.append("ellipse").attr("class", "ssMid").style("fill", color);
            return shape.attr("rx", u * .6).attr("ry", v * .6);
        }
        function DrawScalar(g,u,v,color,justUpdate){
            //DrawBorder(g,u,v,color);
            return DrawS_S(g,u,v,color,justUpdate);
        }
        function DrawGrp_Grp(g,u,v,color,justUpdate){
            DrawBorder(g,u,v,color,justUpdate);
            var shape = justUpdate ? g.select(".grpMid") : g.append("rect").attr("class", "grpMid").style("fill", color);
            return shape.attr("x", -u * .6).attr("y", -v * .6).attr("width", u * 1.2).attr("height", v * 1.2);
        }
        function DrawGroup(g,u,v,color,justUpdate){
            return DrawGrp_Grp(g,u,v,color,justUpdate);
        }
        function DrawS_V(g,u,v,color,justUpdate){
            var shape = justUpdate ? g.select(".svMid") : g.append("rect").attr("class", "svMid").style("fill", color);
            return shape.attr("x", -u).attr("y", -v * .3).attr("width", u * 2).attr("height", v * .6);
        }
        function DrawV_S(g,u,v,color,justUpdate){
            var shape = justUpdate ? g.select(".vsMid") : g.append("rect").attr("class", "vsMid").style("fill", color);
            return shape.attr("x", -u * .3).attr("y", -v).attr("width", u * .6).attr("height", v * 2);
        }
        function DrawVector(g,u,v,color,justUpdate){
            var shape = justUpdate ? g.select(".vMid") : g.append("rect").attr("class", "vMid").style("fill", color);
            return shape.attr("x", -u).attr("y", -v).attr("width", u * 2).attr("height", v * 2);
        }
        function DrawV_V(g,u,v,color,justUpdate){
            return DrawVector(g,u,v,color,justUpdate);
        }
        function DrawS_Grp(g,u,v,color,justUpdate){
            DrawBorder(g,u,v,color,justUpdate);
            var shape = justUpdate ? g.select(".sgrpMid") : g.append("rect").attr("class", "sgrpMid").style("fill", color);
            return shape.attr("x", -u *.6).attr("y", -v * .3).attr("width", u * 1.2).attr("height", v * .6);
        }
        function DrawGrp_S(g,u,v,color,justUpdate){
            DrawBorder(g,u,v,color,justUpdate);
            var shape = justUpdate ? g.select(".grpsMid") : g.append("rect").attr("class", "grpsMid").style("fill", color);
            return shape.attr("x", -u * .3).attr("y", -v * .6).attr("width", u * .6).attr("height", v * 1.2);
        }
        function DrawV_Grp(g,u,v,color,justUpdate){
            return DrawGrp_Grp(g,u,v,color,justUpdate);
        }
        function DrawGrp_V(g,u,v,color,justUpdate){
            return DrawGrp_Grp(g,u,v,color,justUpdate);
        }



        var zoomedElement = root = % s;


        var conns = % s;


        var zoomedElement0 = root;
        var lastRightClickedElement = root,
            lastRightClickedElementIndex = 0;

        var enterIndex = 0,
            exitIndex = 0;

        var lastLeftClickedElement = root,
            leftClickIsForward = true,
            lastClickWasLeft = true;

        ExpandColonVars(root);
        InitTree(root, null, 1);
        columnWidthsPx.length = maxDepth + 1; // since depth is one based
        columnLocationsPx.length = maxDepth + 1; // since depth is one based
