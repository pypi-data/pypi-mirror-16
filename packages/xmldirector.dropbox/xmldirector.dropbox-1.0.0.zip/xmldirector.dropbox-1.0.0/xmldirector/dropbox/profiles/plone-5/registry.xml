<?xml version="1.0"?>
<registry>
    <records prefix="plone.resources/xmldirector-dropbox"
              interface='Products.CMFPlone.interfaces.IResourceRegistry'>
       <value key="js">++resource++xmldirector.dropbox/local.js</value>
       <value key="css">
          <element>++resource++xmldirector.dropbox/styles.css</element>
       </value>
    </records>

    <records prefix="plone.bundles/xmldirector-dropbox"
              interface='Products.CMFPlone.interfaces.IBundleRegistry'>
      <value key="resources" purge="false">
        <element>xmldirector-dropbox</element>
      </value>
      <value key="csscompilation">++resource++xmldirector.dropbox/styles.css</value>
      <value key="jscompilation">++resource++xmldirector.dropbox/local.js</value>
      <value key="enabled">True</value>
      <value key="compile">True</value>
      <value key="merge_with">default</value>
    </records>
</registry>
