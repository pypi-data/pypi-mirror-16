[{{ PROJECT_NAME }}]
processes = {{ PROJECT_NAME }}-gunicorn:/usr/bin/{{ PROJECT_NAME }}-gunicorn
{% if USE_CELERY %}    {{ PROJECT_NAME }}-celery:/usr/bin/{{ PROJECT_NAME }}-celery worker
{% endif %}extra_postinst3 = postinst.sh
username = {{ PROJECT_NAME }}

[djangofloor]

[anyjson]
pre_source = debtools.multideb.remove_tests_dir

[requests-oauthlib]
pre_source = debtools.multideb.remove_tests_dir

[celery]
pre_source = djangofloor.deb_fixes.fix_celery

[Django]
pre_source = djangofloor.deb_fixes.fix_django

[pathlib]
pre_source = djangofloor.deb_fixes.fix_pathlib

[msgpack-python]
{% if use_python3 %}Package3 = python3-msgpack
{% else %}Package = python-msgpack
{% endif %}pre_source = djangofloor.deb_fixes.fix_msgpack

{% if use_python3 %}[python3-openid]
Package3 = python3-openid
{% else %}[python-openid]
Package = python-openid
{% endif %}

